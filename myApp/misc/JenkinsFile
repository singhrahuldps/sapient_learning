node {
   
   stage('Preparation') {
     git branch: 'week1', url: 'https://github.com/singhrahuldps/sapient_learning.git'
   }

   stage('Build') {
    bat 'cd myApp'
    bat 'mvn -Dmaven.test.failure.ignore clean install'
   }

   stage('Results') {
     junit 'target/surefire-reports/**/*.xml'
     archive 'target/*.jar'
   }

}